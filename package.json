{
  "name": "ngx-contextmenu-platform",
  "version": "10.0.0",
  "description": "An Angular component to show a context menu on an arbitrary component",
  "keywords": [
    "angular2",
    "contextmenu",
    "angular2-contextmenu",
    "ngx-contextmenu",
    "ngx",
    "ng2",
    "ng2-contextmenu"
  ],
  "author": "Isaac Mann <isaacplmann@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/isaacplmann/ngx-contextmenu.git"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:demo",
    "build:demo": "rimraf dist/demo && ng build",
    "build:lib": "ng build ngx-contextmenu --prod && copyfiles README.md LICENSE dist/lib/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pub": "npm run pub:lib && npm run pub:demo",
    "pub:demo": "npm run build:demo -- --base-href /ngx-contextmenu/ && gh-pages -d dist/demo",
    "pub:lib": "npm run build:lib && npm publish dist/lib",
    "pack:lib": "npm run build:lib && cd dist/lib && npm pack && copyfiles *.tgz ../../ && cd ../.. && rimraf dist/lib/*.tgz"
  },
  "private": false,
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.17",
    "@angular-devkit/build-ng-packagr": "^0.1100.0-next.5",
    "@angular-devkit/core": "^11.2.17",
    "@angular/cdk": "^10.1.3",
    "@angular/cli": "^10.0.5",
    "@angular/common": "^10.0.8",
    "@angular/compiler": "^10.0.8",
    "@angular/compiler-cli": "^10.0.8",
    "@angular/core": "^10.0.8",
    "@angular/platform-browser": "^10.0.8",
    "@angular/platform-browser-dynamic": "10.0.8",
    "@angular/router": "^10.0.8",
    "@types/node": "~14.6.0",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "gh-pages": "^3.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.0.4",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "ts-node": "~8.10.2",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.1",
    "tslint": "~6.1.3",
    "typescript": "~3.9.7",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  }
}
